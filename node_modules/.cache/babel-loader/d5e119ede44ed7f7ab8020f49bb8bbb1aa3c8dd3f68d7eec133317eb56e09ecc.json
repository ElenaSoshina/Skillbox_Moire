{"ast":null,"code":"export default {\n  props: {\n    quantity: Number\n  },\n  computed: {\n    computedQuantity: {\n      get() {\n        return this.quantity;\n      },\n      set(value) {\n        if (value >= 0) {\n          this.$emit('update:quantity', Number(value));\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["props","quantity","Number","computed","computedQuantity","get","set","value","$emit"],"sources":["src/components/Product/ProductCounter.vue"],"sourcesContent":["<template>\n  <!-- eslint-disable  -->\n  <div class=\"form__counter\">\n    <button type=\"button\" aria-label=\"Убрать один товар\" @click.prevent=\"computedQuantity--\">\n      <svg width=\"12\" height=\"12\" fill=\"currentColor\">\n        <use xlink:href=\"#icon-minus\"></use>\n      </svg>\n    </button>\n\n    <input type=\"number\" v-model=\"computedQuantity\" />\n\n    <button type=\"button\" aria-label=\"Добавить один товар\" @click.prevent=\"computedQuantity++\">\n      <svg width=\"12\" height=\"12\" fill=\"currentColor\">\n        <use xlink:href=\"#icon-plus\"></use>\n      </svg>\n    </button>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    quantity: Number,\n  },\n  computed: {\n    computedQuantity: {\n      get() {\n        return this.quantity;\n      },\n      set(value) {\n        if (value >= 0) {\n          this.$emit('update:quantity', Number(value));\n        }\n      },\n    },\n  },\n};\n</script>\n"],"mappings":"AAoBA;EACAA,KAAA;IACAC,QAAA,EAAAC;EACA;EACAC,QAAA;IACAC,gBAAA;MACAC,IAAA;QACA,YAAAJ,QAAA;MACA;MACAK,IAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAC,KAAA,oBAAAN,MAAA,CAAAK,KAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}